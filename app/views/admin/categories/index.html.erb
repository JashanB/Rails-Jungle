
<section class="products-index">


  <header class="page-header">
    <h1>Categories</h1>
  </header>
  <div class="well">
  <%= link_to '+ New Category', [:new, :admin, :category], class: 'btn btn-info' %>
  </div>
  <div class="panel panel-default products">

    <table class="table table-bordered">
      <thead>
        <tr>Categories</tr>
      </thead>
      <tbody>
        <%= render @categorys %>
      </tbody>
    </table>

  </div>
</section>


<tr>
  <td>
    <%= image_tag line_item.product.image.tiny %>
  </td>
  <td>
    <h4><%= line_item.product.name %></h4>
  </td>
  <td>
    <%= humanized_money_with_symbol line_item.item_price_cents / 100 %>
  </td>
  <td>
  <h4><%= line_item.quantity %></h4>
</td>
  <td>
    <%= humanized_money_with_symbol line_item.item_price_cents / 100 * line_item.quantity %>
  </td>
</tr>
